{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "no-any": true,
        "no-console": false,
        "max-line-length": [
            true,
            180
        ],
        "ban-types": [
            false,
            [
                "Function"
            ]
        ]
    },
    "rulesDirectory": []
}